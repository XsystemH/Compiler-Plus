int i = 1;
int j = i + 2;

class A {
    int x;
    int y;

    int foo() {
        return x + 1;
    }
};

int f(int x) {
    return x + 1;
}

int main() {
    int[][][] arr = new int[2][3][10];
    A a;
    a.x = 1;
    if (a.x > 1) {
        a.y = a.x - 1;
    } else {
        a.y = a.x + 1;
    }

    for (int k = 0; k < 10; k++) {
        i++;
    }
    string s = f"abc$2$def$1 + 2$ghi";
    int[] c = {1, 2, 3};
    int d = s.length();
    return 0;
